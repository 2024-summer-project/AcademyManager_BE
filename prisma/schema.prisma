generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// TODO: 임시유저 => db schema 에 맞게 변환 해야함.
model User {
  id    Int     @id @default(autoincrement())
  user_id String @unique
  academy_id String @unique
  email String @unique
  birth_date DateTime
  user_name String
  password String
  phone_number String
  role Role @default(STUDENT)
}

enum Role{
  CHIEF
  TEACHER
  PARENT
  STUDENT
}